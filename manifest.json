{
    "name": "ヒラケヨハテブ",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "現在開いてるタブのはてなブックマークページを、新しいタブに開く",
    "icons": {
        "32": "img/icon_32.png",
        "128": "img/icon_128.png"
    },
    "browser_action": {
        "default_icon": "img/icon_32.png",
        "default_title": "はてなブックマークを確認する"
    },
    "background": {
        "scripts": ["js/background.js"]
    },
    "permissions": [
        "tabs"
    ]
  }